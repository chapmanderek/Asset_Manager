<p id="notice"><%= notice %></p>

<h1>Listing E Assets</h1>

<table>
  <thead>
    <tr>
      <th>Tag</th>
      <th>Staff</th>
      <th>Type</th>
      <th>Make</th>
      <th>Location</th>
      <th>Model</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @e_assets.each do |e_asset| %>
      <tr>
        <td><%= e_asset.tag_id %></td>
        <td><%= e_asset.staff %></td>
        <td><%= e_asset.e_type %></td>
        <td><%= e_asset.make %></td>
        <td><%= e_asset.location %></td>
        <td><%= e_asset.model %></td>
        <td><%= link_to 'Show', e_asset %></td>
        <td><%= link_to 'Edit', edit_e_asset_path(e_asset) %></td>
        <td><%= link_to 'Destroy', e_asset, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New E asset', new_e_asset_path %>
